{
    "manifest_version": 3,
    "name": "Webpage Narrator",
    "version": "0.01",
    "description":"Narrate Webpage",
    "background": {
        "service_worker": "background.js"
    },
    "permissions":["storage", "activeTab", "scripting"],
    "action": {
        "default_popup":"popup.html",
        "default_icon":{
            "32":"icon.png"
        }
    },
    "icons": {
        "32":"icon.png"
    }
    
}